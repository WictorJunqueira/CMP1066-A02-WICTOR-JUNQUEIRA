OH O HOME AI PO (MONSTRO MEDIA(MONSTRO A, MONSTRO B))
	
	BORA CUMPADE (A + B)/2;
BIRL

HORA DO SHOW 

	CE QUER VER ESSA PORRA? ("DIGITA AI SUA N1-1 FRANGO, PORRA"); 
	MONSTRO N1-1;
	QUE QUE CE QUER MONSTRAO? ("%d", &N1-1);

	CE QUER VER ESSA PORRA? ("DIGITA AI SUA N1-2 FRANGO, PORRA");
	MONSTRO N1-2;
	QUE QUE CE QUER MONSTRAO? ("%d", &N1-2);

	CE QUER VER ESSA PORRA? ("DIGITA AI SUA N2-1 FRANGO, PORRA");
	MONSTRO N2-1;
	QUE QUE CE QUER MONSTRAO? ("%d", &N2-1);

	CE QUER VER ESSA PORRA? ("DIGITA AI SUA N2-1 FRANGO, PORRA");
	MONSTRO N2-2;
	QUE QUE CE QUER MONSTRAO? ("%d", &N2-2);

	
	MONSTO N1 = AJUDA O MALUCO TA DOENTE MEDIA(N1-1, N1-2);
	MONSTO N2 = AJUDA O MALUCO TA DOENTE MEDIA(N2-1, N2-2);


	MEDIA-FINAL = (N1*0,4)+(N2*0,6);


	ELE QUE A GENTE QUER? (MEDIA-FINAL >= 5)

		CE QUER VER ESSA PORRA? ("VOCÃŠ CONSTRUIU FIBRA MANO");

	NAO VAI DAR NAO
	
		CE QUER VER ESSA PORRA? ("PORRA ! FEZ AGUA COM MUSCULO");

